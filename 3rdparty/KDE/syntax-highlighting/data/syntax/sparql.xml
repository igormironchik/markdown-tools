<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE language>
<language name="SPARQL" section="Database" version="1" kateversion="5.0"
    extensions="*.rq;*.sparql"
    mimetype="application/sparql-query"
    author="Damian Oswald (damian.oswald@protonmail.com)"
    license="MIT">
