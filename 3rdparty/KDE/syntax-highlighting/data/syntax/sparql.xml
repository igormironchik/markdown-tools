      <item>DISTINCT</item>
      <item>DROP</item>
      <item>EXISTS</item>
      <item>FILTER</item>
      <item>FROM</item>
      <item>GRAPH</item>
      <item>GROUP</item>
      <item>HAVING</item>
      <item>IN</item>
      <item>INSERT</item>
      <item>INTO</item>
      <item>LIMIT</item>
      <item>LOAD</item>
      <item>MINUS</item>
